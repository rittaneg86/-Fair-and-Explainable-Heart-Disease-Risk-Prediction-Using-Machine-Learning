@article{park2012are,
  title = {Are Brain Networks Stable during a 24-Hour Period?},
  author = {Park, Bumhee and Kim, Joong Il and Lee, Dongha and Jeong, Seok Oh and Lee, Jong Doo and Park, Hae Jeong},
  year = 2012,
